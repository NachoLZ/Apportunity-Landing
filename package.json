{
  "name": "apportunity-landing",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "clean": "rimraf build public/build dist",
    "dev": "remix build && run dev:*",
    "dev:node": "cross-env NODE_ENV=development nodemon ./build/index.js --watch ./build/index.js",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "postinstall": "remix setup node",
    "bundle": "npm run build && npm run bundle:esbuild",
    "bundle:esbuild": "esbuild --bundle build/index.js --platform=node --outfile=dist/index.js",
    "start": "remix-serve ./build/index.js",
    "build-static": "wget -r -nH -i static-urls.txt -P static",
    "serve-static": "serve static",
    "build": "remix build"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "emailjs-com": "^3.2.0",
    "esbuild": "^0.17.11",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "esbuild": "^0.17.11",
    "eslint": "^8.38.0",
    "prettier": "3.2.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
