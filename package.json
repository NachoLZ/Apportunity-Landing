{
  "name": "Apportunity",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "cross-env  npm-run-all --parallel dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run  ",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css ",
    "dev:remix": "cross-env NODE_ENV=development remix",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "insertSimulatorData": "ts-node insertSimulatorData.ts",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate",
    "start": "node -r dotenv/config  NODE_ENV=production",
    "google": "cross-env NODE_ENV=production node -r  ",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
